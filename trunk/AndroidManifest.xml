<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.android.picasaphotouploader">
  <application android:label="@string/app_name" android:icon="@drawable/uploader">
    <activity android:name=".PicasaPhotoUploader" android:label="Picasa Photo Uploader">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".EditPreferences" android:label="Picasa Photo Uploader">
